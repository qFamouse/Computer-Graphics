<Page x:Class="ComputerGraphics.UI.Pages.Clipping"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ComputerGraphics.UI.Pages"
      xmlns:viewModels="clr-namespace:ComputerGraphics.UI.ViewModel"
      mc:Ignorable="d" 
      d:DesignHeight="400" d:DesignWidth="170"
      Title="Amputation"
      Background="White">
    <Page.DataContext>
        <viewModels:ClippingViewModel/>
    </Page.DataContext>
    <StackPanel>
        <Button Command="{Binding SpacePreparingCommand}">Space Preparing</Button>
    </StackPanel>
</Page>
